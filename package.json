{
  "name": "jinge-demos",
  "version": "2.0.0",
  "description": "demos for jinge web framework",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinge-design/jinge-demos.git"
  },
  "scripts": {
    "build": "bun run build.ts",
    "lint-fix": "eslint 'packages/**/*.{js,mjs,ts,tsx}' --fix && prettier ./packages --log-level=warn --write",
    "lint": "eslint '**/*.{js,mjs,ts,tsx}' && prettier --check .",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "keywords": [
    "jinge",
    "demo"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jinge-design/jinge-demos/issues"
  },
  "homepage": "https://github.com/jinge-design/jinge-demos#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/bun": "^1.1.10",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.4",
    "bun": "^1.1.29",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.3.1",
    "husky": "^9.1.6",
    "jinge-compiler": "4.0.0-alpha.9",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "jinge": "4.0.0-alpha.8",
    "jinge-router": "4.0.0-alpha.4"
  }
}
