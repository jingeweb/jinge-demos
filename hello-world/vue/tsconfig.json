{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tsbuildinfo",
    "composite": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "include": ["src", "vite.config.ts", "*.js", "*.mjs"]
}
