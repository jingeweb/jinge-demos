{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tsbuildinfo",
    "composite": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "strictNullChecks": true,
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "./"
  }
}
